{
  "expo": {
    "name": "bill-express-m",
    "slug": "bill-express-m",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-b.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-b.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-b.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.perohub.billexpressm"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash-b.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8e578d82-fa7d-4a92-abe8-d63d4c30bb9c"
      }
    }
  }
}
